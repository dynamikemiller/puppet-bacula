JobDefs {
    Name     = <%= @name %>
    Type     = <%= @jobtype %>
    Pool     = <%= @pool %>
    Schedule = <%= @sched %>
    Messages = <%= @messages %>
    Priority = <%= @priority %>
    Accurate = <%= @accurate %>
<% if @level -%>
    Level    = <%= @level %>
<% end -%>
<% if @full_backup_pool -%>
    Full Backup Pool = <%= @full_backup_pool %>
<% end -%>
<% if @differential_backup_pool -%>
    Differential Backup Pool = <%= @differential_backup_pool %>
<% if @write_bootstrap -%>
    Write Bootstrap = <%= @write_bootstrap %>
<% end -%>
<%= scope.function_template(['bacula/_job_reschedule.erb']) -%>
}
